{
  "comments": {
    "lineComment": "//",
    "blockComment": [ "/*", "*/" ]
  },
  "brackets": [
    ["[", "]"]
  ],
  "autoClosingPairs": [
    ["[", "]"],
    ["\"", "\""]
  ],
  "surroundingPairs": [
    ["[", "]"],
    ["\"", "\""]
  ],
  "folding": {
    "markers": {
      "start": "^\\s*(#org)\\b",
      "end": "^\\s*(end|return|goto|gotostd|killscript)\\b"
    }
  },
  "wordPattern": "(#|@)?\\w+"
}