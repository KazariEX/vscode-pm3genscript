{
  "name": "pm3gen-script-syntax",
  "displayName": "pm3gen-script-syntax",
  "description": "",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.54.0"
  },
  "categories": [
    "Other"
  ],
  "main": "./client/out/extension",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -b",
    "watch": "tsc -b -w"
  },
  "contributes": {
    "languages": [
      {
        "id": "pm3genscript",
        "aliases": [
          "Pokemon Third Generation Script",
          "pm3genscript"
        ],
        "extensions": [
          ".pts"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "pm3genscript",
        "scopeName": "source.pm3genscript",
        "path": "./syntax.json"
      }
    ],
    "snippets": [
      {
        "language": "pm3genscript",
        "path": "./snippets.json"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^20.8.2",
    "@types/vscode": "^1.82.0"
  }
}
